  <script>
    // Helper: cepat ganti gambar contoh via query string ?img=URL
    (function(){
      const params=new URLSearchParams(location.search);
      const img=params.get('img');
      if(img){ document.getElementById('exampleBio').src=img; }
    })();

    // === JAVASCRIPT UNTUK ANIMASI ON-SCROLL ===
    document.addEventListener('DOMContentLoaded', function() {
      const cards = document.querySelectorAll('.card');

      // Cek jika browser mendukung IntersectionObserver
      if ('IntersectionObserver' in window) {
        const observer = new IntersectionObserver((entries, observer) => {
          entries.forEach(entry => {
            // Jika elemen (kartu) masuk ke dalam viewport
            if (entry.isIntersecting) {
              entry.target.classList.add('is-visible');
              // Hentikan pengamatan setelah animasi berjalan agar tidak berulang
              observer.unobserve(entry.target);
            }
          });
        }, {
          threshold: 0.1 // Animasi aktif saat 10% dari kartu terlihat
        });

        // Amati setiap kartu
        cards.forEach(card => {
          observer.observe(card);
        });
      } else {
        // Fallback untuk browser lama yang tidak mendukung IntersectionObserver
        // Tampilkan semua kartu secara langsung
        cards.forEach(card => {
          card.classList.add('is-visible');
        });
      }
    });

  </script>
